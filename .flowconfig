[ignore]

[include]

[libs]
flow-typed/

[lints]
all=error
dynamic-export=off
sketchy-null-bool=off
sketchy-null-number=off
sketchy-null-string=off
sketchy-null-mixed=off

[options]
include_warnings=true

[strict]
nonstrict-import
unclear-type
unsafe-getters-setters
untyped-import
untyped-type-import
